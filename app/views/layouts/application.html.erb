<!DOCTYPE html>
<html>
  <head>
   <title><%= full_title(yield(:title)) %></title>
    <%= render 'layouts/head' %>
    <%= render 'layouts/for_ie' %>
  </head>
  <body>
   <%= render 'layouts/header' %>
   <div class="container">
    <% flash.each do |message_type, message| %>
     <%= content_tag(:div, message, class: "flash-margin alert alert-#{message_type}") %>
      <% end %>
      <%= yield %>
     <%#= debug(params) if Rails.env.development? %>
    <%= render 'layouts/footer' %>
   </div>
  </body>
</html>